/*
Leia uma data e determine se ela e v√°lida. Ou seja, verifique se o m√™s est√° entre 1 e 12, e 
se o dia existe naquele m√™s. Note que Fevereiro tem 29 dias em anos bissextos, e 28 dias em 
anos nao bissextos.
*/
#include <stdio.h>

int main()
{
    int dia = 0;
    int mes = 0;
    int ano = 0;

    printf("Digite o dia: ");
    scanf("%d", &dia);

    printf("Digite o m√™s: ");
    scanf("%d", &mes);

    printf("Digite o ano: ");
    scanf("%d", &ano);

    if (mes < 1 || mes > 12) {
        printf("M√™s inv√°lido.");
    } else if (dia < 1 || dia > 31) {
        printf("Dia inv√°lido.");
    } else if (mes == 4 || mes == 6 || mes == 9 || mes == 11) {
        if (dia > 30) {
            printf("Dia inv√°lido.");
        } else {
            printf("Data v√°lida.");
        }
    } else if (mes == 2) {
        if (ano % 4 == 0 && ano % 100 != 0 || ano % 400 == 0) {
            if (dia > 29) {
                printf("Dia inv√°lido.");
            } else {
                printf("Data v√°lida.");
            }
        } else {
            if (dia > 28) {
                printf("Dia inv√°lido.");
            } else {
                printf("Data v√°lida.");
            }
        }
    } else {
        printf("Data v√°lida.");
    }

    return 0;
}

