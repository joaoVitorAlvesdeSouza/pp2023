Joao vitor Alves de Souza - 2022.
*/


#include <stdio.h>

int main()
{
     float angul = 0;  // usuario vai digitar
     float radi = 0; // resultado
     
    printf("qual o angulo? ");
    scanf("%f", &angul);
    
       radi = angul*3.141592/180 ;
    
    printf("o radiano e: %.2f", radi);
    
    return 0;
}

